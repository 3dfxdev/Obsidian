#!/bin/bash

sed -i -r {s/"!="/"~="/} $1
sed -i -r {s/"0$"/"0,"/} $1
sed -i -r {s/"1$"/"1,"/} $1
sed -i -r {s/"2$"/"2,"/} $1
sed -i -r {s/"3$"/"3,"/} $1
sed -i -r {s/"4$"/"4,"/} $1
sed -i -r {s/"5$"/"5,"/} $1
sed -i -r {s/"6$"/"6,"/} $1
sed -i -r {s/"7$"/"7,"/} $1
sed -i -r {s/"8$"/"8,"/} $1
sed -i -r {s/"9$"/"9,"/} $1
sed -i -r {s/"\"$"/"\","/} $1
sed -i -r {s/"},$"/"}"/} $1

